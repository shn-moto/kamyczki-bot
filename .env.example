# Telegram Bot Token (required)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# PostgreSQL (defaults work with docker-compose)
POSTGRES_USER=kamyczki
POSTGRES_PASSWORD=kamyczki_secret
POSTGRES_DB=kamyczki_bot
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Web server
WEB_PORT=8080

# Mini App URL (set after cloudflared starts, or leave empty)
# Check: docker logs kamyczki-tunnel | grep https://
WEBAPP_BASE_URL=
